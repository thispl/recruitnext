 "html": "<div class=\"container\">\n    <div class=\"row\">\n         \n                <div class=\"col-xs-3\">\n                {% set letter_head = frappe.get_doc(\"Letter Head\",\"HR\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n               </div>\n                 <div class=\"col-xs-6\">\n                  <h6><b><center>www.selcohr.com</center></b></h6>\n                  <h6><center><b>Ref No : </b></center></h6>\n                  <h6><center><b>Resume of {{doc.given_name__surname or \"\"}} </b></center></h6>\n                 </div>\n                  <div class=\"col-xs-3\">\n                 <Img scr=\"{{doc.candidate_image}}\" height=\"100\" width=\"80\">\n                 </div>\n        <div class=\"col-xs-12\">\n            \n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Preference</u></b></left></th>\n                 </tr>\n                 </table>\n             </div>\n                <div class=\"col-xs-3\">\n                <b>Job Title(s)</b><br>\n                <b>Job Industry</b><br>\n                <b>Expected Salary</b><br>\n                <b>Location </b>\n            </div>\n                 <div class=\"col-xs-8\">\n                  <b>:</b><br>\n                  <b>:</b><br>\n                  <b>:</b>{{doc.expected_ctc or \"\"}}<br>\n                  <b>:</b>{{doc.territory or \"\"}}<br>\n            </div>\n       </div>\n          <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Personal Details</u></b></left></th>\n                 </tr>\n                 </table>  \n          </div>\n          <div class=\"col-xs-3\">\n                <b>Name</b><br>\n                <b>Full Name</b><br>\n                <b>Date of Birth</b><br>\n                <b>Gender</b><br>\n                <b>\tNationality\t</b>\n                \n                \n            </div>\n             <div class=\"col-xs-3\">\n             <b>:</b>{{doc.given_name__surname or \"\"}}<br>\n              <b>:</b>{{doc.full_name or \"\"}}<br>\n              <b>:</b>{{frappe.utils.formatdate(doc.date_of_birth) or \"\"}}<br>\n              <b>:</b>{{doc.gender or \"\"}}<br>\n              <b>:</b>{{doc.nationality or \"\"}}\n              \n              \n            </div>\n             <div class=\"col-xs-3\">\n                <b>Initials</b><br>\n                <b>Title</b><br>\n                <b>Age</b><br>\n                <b>Marital<br> Status</b><br>\n                <b></b>\n            </div>\n             <div class=\"col-xs-3\">\n                <b>:</b>{{doc.sur_name or \"\"}}<br>\n                <b>:</b><br>\n                <b>:</b>{{doc.age or \"\"}}<br>\n                <b>:</b>{{doc.civil_status or \"\"}}\n            </div>\n          </div>\n           <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Travel & Other Identification Documents</u></b></left></th>\n                 </tr>\n                 </table>  \n          </div>\n          <div class=\"col-xs-3\">\n                <b>Passport(s)</b><br><br>\n                <b>Country</b><br>\n                  {{doc.nationality or \"\"}}</div>\n                    <div class=\"col-xs-3\">\n                <br><br>\n                <b>Passport Number</b><br>\n                {{doc.passport_no or \"\"}}\n                  </div>\n                    <div class=\"col-xs-3\">\n                <br><br>\n                <b>Expiry Date</b><br>\n                {{frappe.utils.formatdate(doc.expiry_date) or \"\"}}\n                  </div>\n            </div>\n             <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Working Experience</u></b></left></th>\n                 </tr>\n                 </table>  \n                   <table class=\"table table-bordered\" border=\"1\">\n                       <tr>\n                           <th colspan=\"\"><b><center>Organization</center></b></th>\n                            <th colspan=\"\"><b><center>Role</center></b></th>\n                              <th colspan=\"\"><b><center>From Date</center></b></th>\n                                <th colspan=\"\"><b><center>To Date</center></b></th>\n                                  <th colspan=\"\"><b><center>Reason for Leaving</center></b></th>\n                                   <th colspan=\"\"><b><center>Work Summary</center></b></th>\n                                   {%- for row in doc.experience -%}\n                                  \n                       </tr>\n                       <tr>\n                           <td colspan=\"\"><center>{{row.organization or \"\"}}</center></td>\n                            <td colspan=\"\"><center>{{row.role or \"\"}}</center></td>\n                             <td colspan=\"\"><center>{{frappe.utils.formatdate(row.from_date) or \"\"}}</center></td>\n                              <td colspan=\"\"><center>{{frappe.utils.formatdate(row.to_date) or \"\"}}</center></td>\n                               <td colspan=\"\"><center>{{row.reason_for_leaving or \"\"}}</center></td>\n                                <td colspan=\"\"><center>{{row.work_summary or \"\"}}</center></td>\n                       </tr>\n                       {%- endfor -%}\n                       </table>\n                 \n          </div>\n      </div>\n        <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Academic / Educational Qualifications</u></b></left></th>\n                 </tr>\n                 </table>  \n                 <table class=\"table table-bordered\" border=\"1\">\n                     <tr>\n                     <th colspan=\"\"><center>School/University</center></th>\n                       <th colspan=\"\"><center>Qualification</center></th>\n                         <th colspan=\"\"><center>Specialization</center></th>\n                           <th colspan=\"\"><center>Level</center></th>\n                             <th colspan=\"\"><center>Year of Passing</center></th>\n                               <th colspan=\"\"><center>Course Type</center></th>\n                                 <th colspan=\"\"><center>Class / Percentage</center></th>\n                                   <th colspan=\"\"><center>Country</center></th>  \n                                      {%- for row in doc.education -%}\n                                   \n                     </tr>\n                     <tr>\n                         <td colspan=\"\"><center>{{row.schooluniversity or \"\"}}</center></td>\n                           <td colspan=\"\"><center>{{row.qualification or \"\"}}</center></td>\n                             <td colspan=\"\"><center>{{row.specialization or \"\"}}</center></td>\n                               <td colspan=\"\"><center>{{row.level or \"\" }}</center></td>\n                                 <td colspan=\"\"><center>{{row.year_of_passing or \"\"}}</center></td>\n                                   <td colspan=\"\"><center>{{row.course_type or \"\"}}</center></td>\n                                     <td colspan=\"\"><center>{{row.class__percentage or \"\"}}</center></td>\n                                       <td colspan=\"\"><center>{{row.country or \"\"}}</center></td>\n                     </tr>\n                     {%- endfor -%}\n                     </table>\n          </div>\n           \n           \n          </div>\n                     \n        <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Professional / Technical / Other Qualifications</u></b></left></th>\n                 </tr>\n                 </table>  \n                      <table class=\"table table-bordered\" border=\"1\">\n                     <tr>\n                         <th colspan=\"\"><center>Name of Institute</center></th>\n                          <th colspan=\"\"><center>Title of Qualification</center></th>\n                           <th colspan=\"\"><center>Date Awarded</center></th>\n                            <th colspan=\"\"><center>Duration</center></th>\n                             <th colspan=\"\"><center>Place</center></th>\n                              <th colspan=\"\"><center>Country</center></th>\n                               {%- for row in doc.professional_qualification -%}\n                     </tr>\n                     <tr>\n                         <td colspan=\"\"><center>{{row.name_of_institute or \"\"}}</center></td>\n                          <td colspan=\"\"><center>{{row.title_of_qualification or \"\"}}</center></td>\n                           <td colspan=\"\"><center>{{frappe.utils.formatdate(row.date_awarded) or \"\"}}</center></td>\n                            <td colspan=\"\"><center>{{row.duration or \"\"}}</center></td>\n                             <td colspan=\"\"><center>{{row.place or \"\"}}</center></td>\n                              <td colspan=\"\"><center>{{row.country or \"\"}}</center></td>\n                     </tr>\n                     {%- endfor -%}\n                     </table>\n          </div>\n           \n           \n </div>\n               \n        <div class=\"row\">\n               <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"12\"><left><b><u>Language Proficiency</u></b></left></th>\n                 </tr>\n                 </table>  \n                   <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"\"><center>Language</center></th>\n                     <th colspan=\"\"> <center>Read</center></th>\n                     <th colspan=\"\"><center>Write</center></th>\n                     <th colspan=\"\"><center>Speak</center></th>\n                      {%- for row in doc.languages_known -%}\n                </tr>\n                <tr>\n                    <td colspan=\"\"><center>{{row.language or \"\"}}</center></td>\n                      <td colspan=\"\"><center>\n                       {% if row.read == 1 %}Yes{% else %}No{% endif %}</center></td>\n                        <td colspan=\"\"><center>{% if row.write == 1 %}Yes{% else %}No{% endif %}</center></td>\n                          <td colspan=\"\"><center>{% if row.speak == 1 %}Yes{% else %}No{% endif %}</center></td>\n                </tr>\n                {%- endfor -%}\n                </table>\n          </div>\n          </div>\n  </div>",
